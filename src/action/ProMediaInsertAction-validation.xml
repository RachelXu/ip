<!DOCTYPE validators PUBLIC
        "-//Apache Struts//XWork Validator 1.0.2//EN"
        "http://struts.apache.org/dtds/xwork-validator-1.0.2.dtd">
<validators>
    <field name="media.mediaName">
        <field-validator type="requiredstring">
            <message>请输入节目中文名称</message>
        </field-validator>
    </field>
    <field name="media.mediaNameEn">
        <field-validator type="requiredstring">
            <message>请输入节目英文名称</message>
        </field-validator>
    </field>
</validators>
